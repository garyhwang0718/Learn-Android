<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.sjq.myviewtest.MainActivity">

    <com.sjq.myviewtest.View.MyView
        android:layout_width="wrap_content"
        android:id="@+id/m1"
        android:layout_height="wrap_content"
        android:background="@color/colorAccent"
        android:text="343"/>

    <com.sjq.myviewtest.View.MyView
        android:layout_width="250dp"
        android:layout_height="250dp"
        android:layout_below="@id/m1"
        android:id="@+id/m2"
        android:background="@color/colorAccent"
        android:text="343"/>
    <com.sjq.myviewtest.View.MyView
    android:layout_width="10dp"
    android:layout_height="10dp"
    android:layout_below="@id/m2"
    android:id="@+id/m3"
    android:background="@color/colorAccent"
    android:text="343"/>
    <com.sjq.myviewtest.View.MyView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/m3"
        android:id="@+id/m4"
        app:circle_color="@color/colorPrimary"
        android:text="343"/>
    <com.sjq.myviewtest.View.MyView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/m4"
        android:id="@+id/m5"
        android:text="343"/>
</RelativeLayout>
