<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    >

    <ImageView
        android:id="@+id/feed_item_multiple_image_img"
        android:layout_width="wrap_content"
        android:layout_height="500dp"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/tv"

        android:src="@drawable/ic_launcher" />

    <ImageView
        android:id="@+id/feed_item_multiple_image_video_icon_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:layout_below="@id/tv"
        android:src="@drawable/feed_video_play" />

    <TextView
        android:id="@+id/feed_item_multiple_image_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/feed_item_multiple_image_img"
        android:layout_alignLeft="@id/tv"
        android:maxLines="1"
        android:text="畅听" />

    <TextView
        android:id="@+id/feed_item_multiple_image_desc"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/feed_item_multiple_image_title"
        android:layout_alignLeft="@id/tv"
        android:maxLines="1"
        android:text="畅听" />



</merge>